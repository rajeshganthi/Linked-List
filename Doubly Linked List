class Node{
    int data;
    Node next;
    Node prev;
    
    public Node(int val){
        data = val;
        next = prev = null;
    }
}

class DoublyLL{
    Node head;
    Node tail;
    
    public DoublyLL(){
        head = tail = null;
    }
    
    public void push_back(int val){
        Node newNode = new Node(val);
        if(head==null){
            head = tail = newNode;
        }else{
            newNode.prev = tail;
            tail.next = newNode;
            tail = newNode;
        }
    }
    
    public void push_front(int val){
        Node newNode = new Node(val);
        if(head==null){
            head=tail=newNode;
        }else{
           newNode.next = head;
           head.prev = newNode;
           head = newNode;
        }
    }
    
    public void pop_front(){
        if(head==null){
            System.out.println("List is empty");
            return;
        }
        Node temp = head;
        head = head.next;
        if(head!=null){
            head.prev = null;
        }
        temp.next = null;
    }
    
    public void pop_back(){
        Node temp = tail;
        tail = tail.prev;
        if(tail!=null){
            tail.next = null;
        }
        temp.prev = null;
    }
    
    public void printLL(){
        Node temp = head;
        while(temp!=null){
            System.out.print(temp.data+"<=>");
            temp=temp.next;
        }
        System.out.println("null");
    }
}

class Main{
    public static void main(String[] args){
        DoublyLL dll = new DoublyLL();
        
        dll.push_front(1);
        dll.push_front(2);
        dll.push_front(3);
        dll.printLL();
        
        dll.pop_front();
        dll.printLL();

        dll.push_back(1);
        dll.push_back(2);
        dll.printLL();
    }
}
